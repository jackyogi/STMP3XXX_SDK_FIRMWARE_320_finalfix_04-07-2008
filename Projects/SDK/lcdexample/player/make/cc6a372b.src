	SYMB	TYPE, 259, 'X', 8, #1, 31, 0
	SYMB	GFUN, "DisplayLowBattery", FDisplayLowBattery, #259, 1, 0, 0
	SYMB	ALAB, FDisplayLowBattery, #259
FDisplayLowBattery:
	move	ssh,y:(r7)+
	SYMB	SOFF, 1, 1
	SYMB	SOFF, -1

; 15   |    SysPostMessage(5,LCD_PRINT_RANGE_RSRC,0,(LCD_FIRST_ROW_NUM+2)*LCD_SIZE_ROW,RSRC_BROWNOUT_BITMAP);

	SYMB	LINE, 15
	move	#314,b
	move	b,y:(r7)+
	SYMB	SOFF, 2, 1
	move	#24,b0
	move	b0,y:(r7)+
	SYMB	SOFF, 3, 1
	gmove	#0,b
	move	b,y:(r7)+
	SYMB	SOFF, 4, 1
	move	#$30001,y0
	move	y0,y:(r7)+
	SYMB	SOFF, 5, 1
	move	#5,b0
	move	b0,y:(r7)+
	SYMB	SOFF, 6, 1
	jsr	FSysPostMessage
	move	#5,n7
	nop
	move	(r7)-n7
	SYMB	SOFF, 1, 1

; 16   |
; 17   |}

	SYMB	LINE, 17
	move	y:-(r7),ssh
	SYMB	SOFF, 0, 1
	SYMB	SOFF, 0
	nop
	rts
	SYMB	EFUN
	SYMB	ENDF
	SYMB	TYPE, 260, 'X', 8, #1, 31, 2, #16, #1
	SYMB	ALAB, FSysPostMessage, #260

