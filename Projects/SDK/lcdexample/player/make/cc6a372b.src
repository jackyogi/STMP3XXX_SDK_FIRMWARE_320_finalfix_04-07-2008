; 6    |
; 7    |_reentrant void DisplayClear (void)
; 8    |{

	org	p,".ptextsysdisplay":
	SYMB	LINE, 8
	SYMB	TYPE, 258, 'X', 8, #1, 31, 0
	SYMB	GFUN, "DisplayClear", FDisplayClear, #258, 1, 0, 0
	SYMB	ALAB, FDisplayClear, #258
FDisplayClear:
	move	ssh,y:(r7)+
	SYMB	SOFF, 1, 1
	SYMB	SOFF, -1

; 9    |    SysPostMessage(6,LCD_CLEAR_RANGE,0,0,LCD_SIZE_X,LCD_SIZE_Y);     

	SYMB	LINE, 9
	gmove	#0,a
	move	#159,b0
	move	b0,y:(r7)+
	SYMB	SOFF, 2, 1
	move	#$80,b0
	move	b0,y:(r7)+
	SYMB	SOFF, 3, 1
	gmove	#0,b
	move	b,y:(r7)+
	SYMB	SOFF, 4, 1
	move	a,y:(r7)+
	SYMB	SOFF, 5, 1
	move	#<3,y0
	move	y0,y:(r7)+
	SYMB	SOFF, 6, 1
	move	#6,b0
	move	b0,y:(r7)+
	SYMB	SOFF, 7, 1
	jsr	FSysPostMessage
	move	#6,n7
	nop
	move	(r7)-n7
	SYMB	SOFF, 1, 1

; 10   |}

	SYMB	LINE, 10
	move	y:-(r7),ssh
	SYMB	SOFF, 0, 1
	SYMB	SOFF, 0
	nop
	rts
	SYMB	EFUN

